5
C=float(input("C="))
O=float(input("O="))
H=float(input("H="))
massa_ulushi=C/(C+O+H)*100
print("massa ulushi",massa_ulushi,"%")
4
a=float(input("a="))
b=float(input("b="))
c=float(input("c="))
V=a*b*c
S=2*(a*b+b*c+c*a)
print("V=",V)
print("S=",S)
3
import math
a=float(input("uchburchakning biribchi tomoni: "))
b=float(input("uchburchakning ikkinchi tomoni: "))
c=float(input("uchburchakning biribchi tomoni: "))
p=(a+b+c)/2
S=math.sqrt(p*(p-a)*(p-b)*(p-c))
print("S=",S)
